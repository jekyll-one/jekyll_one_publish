{
  "private": true,
  "name": "utls",
  "version": "2020.0.9",
  "description": "J1 Template Utility Server",
  "homepage": "https://jekyll.one",
  "author": {
    "name": "Juergen Adams",
    "email": "jadams@jekyll.one",
    "url": "https://jekyll.one"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://jekyll.one"
  },
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.6"
  },
  "utilsrv": {
    "enabled": "true",
  },
  "scripts": {
    "clean": "shx rm -f *.lock && shx rm -f package-lock.json",
    "start": "run-s utilsrv-run",
    "develop": "run-s utilsrv",
    "utilsrv": "cross-env UTILSRV_ENABLED=$npm_package_utilsrv_enabled if-env UTILSRV_ENABLED=true && run-s -s utilsrv-run || run-s -s utilsrv-exit",
    "utilsrv-run": "echo Startup utility server .. && node server.js",
    "utilsrv-exit": "echo Startup utility server globally disabled, not started."
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.15.2",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "js-yaml": "^3.13.1",
    "lite-url": "^1.0.5",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "randomstring": "^1.1.5",
    "shx": "^0.2.2",
    "simple-git": "^1.113.0",
    "simple-oauth2": "^1.6.0",
    "sprintf-js": "^1.1.2",
    "touch": "^3.1.0"
  }
}
